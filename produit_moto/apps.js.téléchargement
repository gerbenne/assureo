  $(function () {
    $(".input-datepicker-birth").each(function () {
      $(this).mobiscroll().calendar({
        lang: 'fr',
        display: 'bubble',
        mode: 'mixed',
        scrollLock: false,
        readonly: false,
        defaultValue: new Date(n, m, y),
        onClosed: function () {
          $(this).valid();
        }
      });
    });
  });

