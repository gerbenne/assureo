        let lastScrollTop = 0;
        $(window).scroll(function() {
          let scrollTop = $(this).scrollTop();
          if (scrollTop > lastScrollTop) {
            $('.bottom-nav').stop().fadeOut();
          } else {
            $('.bottom-nav').fadeIn();
          }
          lastScrollTop = scrollTop;
        });

      }

    $(btnMenu).on("click", function () {
